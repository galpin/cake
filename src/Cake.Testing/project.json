{
  "buildOptions": {
    "xmlDoc": true,
    "additionalArguments": [
      "/ruleset:../Cake.ruleset",
      "/additionalfile:../stylecop.json"
    ]
  },
  "dependencies": {
    "Cake.Core": { "target": "project" },
    "StyleCop.Analyzers": "1.0.0",
    "xunit.core": "2.2.0-beta2-build3300"
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETCORE" ]
      },
      "imports": "dnxcore50"
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" },
        "System.Threading.Tasks": { "type": "build" },
        "System.Web": { "type": "build" }
      }
    }
  }
}